{
  "name": "game",
  "version": "0.1.4",
  "description": "Game Service",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LearnersGuild/game.git"
  },
  "author": {
    "name": "Jeffrey Wescott",
    "email": "jeffrey@learnersguild.org"
  },
  "scripts": {
    "addProjectFormationCompleteEvent": "npm run symlinks && ./node_modules/.bin/babel-node scripts/addProjectFormationCompleteEvent",
    "build": "npm run symlinks && mkdir -p dist && webpack --config config/webpack.js",
    "data:github-goals": "npm run symlinks && ./node_modules/.bin/babel-node test/generateTestGoals",
    "data:import:survey-questions": "npm run symlinks && ./node_modules/.bin/babel-node scripts/importSurveyQuestions",
    "data:import:survey-responses": "npm run symlinks && ./node_modules/.bin/babel-node scripts/importSurveyResponses",
    "data:reloadFromFiles": "npm run symlinks && ./node_modules/.bin/babel-node scripts/reloadDataFiles",
    "db:create": "npm run symlinks && ./node_modules/.bin/babel-node scripts/dbCreate.js",
    "db:drop": "npm run symlinks && ./node_modules/.bin/babel-node scripts/dbDrop.js",
    "db:migrate": "npm run symlinks && npm run db:migrate:configure && ./node_modules/.bin/babel-node ./node_modules/.bin/rethink-migrate -r db",
    "db:migrate:configure": "npm run symlinks && ./node_modules/.bin/babel-node scripts/dbConfig.js",
    "db:migrate:create": "./node_modules/.bin/rethink-migrate create -r db",
    "db:migrate:down": "npm run symlinks && npm run db:migrate:configure && ./node_modules/.bin/babel-node ./node_modules/.bin/rethink-migrate -r db down",
    "db:migrate:up": "npm run symlinks && npm run db:migrate:configure && ./node_modules/.bin/babel-node ./node_modules/.bin/rethink-migrate -r db up",
    "db:reset": "npm run db:drop && npm run db:create && npm run db:migrate:up",
    "icons:fetch": "mkdir -p dist && curl -s https://brand.learnersguild.org/icontags > dist/icons-metadata.json",
    "import:projects": "npm run symlinks && ./node_modules/.bin/babel-node scripts/importProjects.js",
    "lint": "./node_modules/.bin/xo",
    "postinstall": "npm run build",
    "preview:projects": "npm run symlinks && ./node_modules/.bin/babel-node scripts/previewProjects",
    "print:levels": "npm run symlinks && ./node_modules/.bin/babel-node scripts/printLevels",
    "print:projects": "npm run symlinks && ./node_modules/.bin/babel-node scripts/printProjects",
    "print:stats": "npm run symlinks && ./node_modules/.bin/babel-node scripts/printStats",
    "print:survey": "npm run symlinks && ./node_modules/.bin/babel-node scripts/printSurveyQuestions",
    "queue:empty": "npm run symlinks && ./node_modules/.bin/babel-node scripts/queueEmpty.js",
    "start": "npm run symlinks && npm run icons:fetch && node server",
    "stats": "npm run symlinks && ./node_modules/.bin/babel-node scripts/runStats",
    "symlinks": "./node_modules/.bin/babel-node ./scripts/createSymlinks",
    "symlinks:remove": "./node_modules/.bin/rimraf ./node_modules/src",
    "test": "npm run symlinks && npm run lint && npm run test:run --",
    "test:ci": "npm run db:create && npm run db:migrate:up && npm run lint && npm run test:cov",
    "test:cov": "npm run test:cov:run && npm run test:cov:send",
    "test:cov:run": "./node_modules/.bin/nyc --reporter=lcov npm run test:run",
    "test:cov:send": "./node_modules/.bin/codeclimate-test-reporter < ./coverage/lcov.info",
    "test:run": "NODE_ENV=test ./node_modules/.bin/mocha $(find . -path './**/__tests__/*.test.js' ! -ipath '*node_modules*')",
    "test:watch": "npm run test:run -- --watch",
    "workers": "npm run symlinks && ./node_modules/.bin/babel-node ./server/workers",
    "workers:cycleLaunched": "npm run symlinks && ./node_modules/.bin/babel-node ./server/workers/cycleLaunched"
  },
  "keywords": [
    "game"
  ],
  "license": "UNLICENSED",
  "private": true,
  "bugs": {
    "url": "https://github.com/LearnersGuild/game/issues"
  },
  "homepage": "https://github.com/LearnersGuild/game#readme",
  "dependencies": {
    "@learnersguild/idm-jwt-auth": "^2.0.1",
    "animal-id": "0.0.1",
    "auto-loader": "^0.2.0",
    "autoprefixer": "^6.5.3",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-runtime": "^6.20.0",
    "basic-auth": "^1.0.4",
    "bluebird": "^3.3.5",
    "bourbon": "^4.2.6",
    "bourbon-neat": "^1.7.4",
    "bull": "^1.0.0-rc1",
    "chokidar": "^1.4.3",
    "config": "^1.21.0",
    "cookie-parser": "^1.4.1",
    "cors": "^2.7.1",
    "css-loader": "^0.26.1",
    "css-modules-require-hook": "^4.0.5",
    "csv-write-stream": "^2.0.0",
    "dotenv": "^2.0.0",
    "elo-rank": "^1.0.0",
    "emailjs": "^1.0.8",
    "express": "^4.13.3",
    "express-graphql": "^0.4.9",
    "express-sslify": "^1.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "google-libphonenumber": "^2.0.6",
    "graphql": "^0.4.18",
    "graphql-custom-types": "^0.3.0",
    "history": "^2.0.0",
    "immutability-helper": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^8.0.4",
    "json-loader": "^0.5.4",
    "jsonwebtoken": "^7.0.1",
    "juration": "0.0.1",
    "keymirror": "^0.1.1",
    "later": "^1.2.0",
    "md5": "^2.1.0",
    "micromustache": "^2.1.26",
    "minimist": "^1.2.0",
    "moment": "^2.12.0",
    "moment-timezone": "^0.5.3",
    "newrelic": "^1.28.2",
    "node-sass": "^3.13.0",
    "normalize.css": "^5.0.0",
    "normalizr": "^2.3.0",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "parse-link-header": "^0.4.1",
    "postcss": "^5.2.6",
    "postcss-loader": "^1.2.0",
    "postcss-modules-extract-imports": "^1.0.1",
    "postcss-modules-local-by-default": "^1.1.1",
    "postcss-modules-scope": "^1.0.2",
    "postcss-modules-values": "^1.2.2",
    "raven": "^0.10.0",
    "raven-js": "^2.1.1",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-toolbox": "^1.3.1",
    "redux": "^3.6.0",
    "redux-auth-wrapper": "^0.9.0",
    "redux-form": "^6.2.1",
    "redux-thunk": "^2.1.0",
    "resolve-url-loader": "^1.6.1",
    "rethink-migrate": "^1.3.1",
    "rethinkdb": "2.3.3",
    "rethinkdb-changefeed-reconnect": "^0.3.1",
    "rethinkdbdash": "2.3.27",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "sass-resources-loader": "^1.1.0",
    "serve-static": "^1.10.0",
    "socketcluster-client": "^4.3.16",
    "socketcluster-server": "^4.1.10",
    "style-loader": "^0.13.1",
    "thinky": "^2.3.7",
    "tmp": "0.0.31",
    "toureiro": "^0.2.12",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-fail-plugin": "^1.0.5",
    "webpack-hot-middleware": "^2.13.2",
    "yamljs": "^0.2.7",
    "yup": "^0.17.1"
  },
  "devDependencies": {
    "babel-plugin-react-transform": "^2.0.2",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "codeclimate-test-reporter": "^0.3.3",
    "enzyme": "^2.6.0",
    "eslint": "^3.11.1",
    "eslint-config-xo-react": "^0.10.0",
    "eslint-plugin-no-use-extend-native": "^0.3.12",
    "eslint-plugin-react": "^6.8.0",
    "factory-girl": "^3.0.1",
    "faker": "^3.1.0",
    "jsdom": "^8.0.2",
    "mocha": "^2.4.5",
    "nock": "^8.0.0",
    "nyc": "^7.1.0",
    "pre-commit": "^1.1.3",
    "react-addons-test-utils": "^15.4.2",
    "react-transform-hmr": "^1.0.4",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "xo": "^0.17.1"
  },
  "engines": {
    "node": "5.6.x",
    "npm": "3.7.x"
  },
  "xo": {
    "extends": "xo-react",
    "rules": {
      "comma-dangle": [
        0
      ],
      "import/named": 1,
      "import/no-dynamic-require": 1,
      "import/no-extraneous-dependencies": 0,
      "import/no-unresolved": 0,
      "import/prefer-default-export": 1,
      "no-mixed-operators": [
        0
      ],
      "no-negated-condition": [
        0
      ],
      "no-unused-vars": [
        2,
        {
          "argsIgnorePattern": "_"
        }
      ],
      "no-use-extend-native/no-use-extend-native": [
        0
      ],
      "object-property-newline": [
        0
      ],
      "quotes": [
        2,
        "single",
        "avoid-escape"
      ],
      "react/forbid-component-props": 0,
      "react/jsx-indent": [
        2,
        2
      ],
      "react/jsx-indent-props": [
        2,
        2
      ],
      "unicorn/filename-case": 0,
      "xo/catch-error-name": 0,
      "xo/filename-case": 0
    },
    "esnext": true,
    "space": true,
    "semicolon": false
  },
  "nyc": {
    "all": true,
    "exclude": [
      "dist/",
      "tmp/"
    ],
    "require": [
      "babel-core/register",
      "babel-polyfill"
    ]
  },
  "precommit": [
    "lint"
  ]
}
